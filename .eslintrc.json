{
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "env": {
    "serviceworker": true,
    "node": true, // This tells ESLint to recognize Node.js global variables like `global`
    "jest": true // This tells ESLint that you are running tests in Jest environment
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["node_modules"]
}
