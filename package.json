{
  "name": "pbem-helperbot",
  "version": "1.0.0",
  "description": "A simple discord bot to help my friends",
  "type": "module",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "wrangler dev src/server.js",
    "test": "jest",
    "fix": "eslint --fix **/*.js",
    "lint": "eslint **/*.js",
    "getCommands": "node src/commandManager/getCommands.js",
    "deleteCommand": "node src/commandManager/deleteCommand.js",
    "prettier": "prettier --write --ignore-unknown",
    "register": "node src/register.js",
    "debug": "wrangler dev --config wrangler-sbx.toml",
    "deploy:prd": "wrangler deploy --config wrangler-prd.toml",
    "deploy:sbx": "wrangler deploy --config wrangler-sbx.toml",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Me",
  "dependencies": {
    "discord-interactions": "^3.4.0",
    "express": "^4.19.2",
    "itty-router": "^4.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "c8": "^10.1.3",
    "chai": "^5.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "mocha": "^11.0.1",
    "nock": "^13.5.6",
    "prettier": "3.4.2",
    "sinon": "^19.0.2",
    "wrangler": "^3.100.0"
  }
}
